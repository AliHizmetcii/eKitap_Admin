@model List<eKitap.Models.Book>
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@{
    ViewData["Title"] = "Detaylar";
    ViewData["Page"] = "classRooms";

    Layout = "~/Views/Shared/_Layout.cshtml";
}


<table class="table">
    <thead>
    <tr>
        <th>
            Kitap İsmi
        </th>
    </tr>
    </thead>
    <tbody>
    @foreach (var item in Model)
    {
        <tr>
            <td>
                <a href="/Books/@item.PdfName" target="_blank">@item.Name</a>
            </td>
        </tr>
    }
    </tbody>
</table>
<a class="btn btn-info" asp-action="Index"><i class="fas fa-backward"></i></a>
